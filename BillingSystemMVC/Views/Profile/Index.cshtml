@model IEnumerable<BillingSystemMVC.BLL.Model.Users.UserProfileDto>
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div>
        @Html.ActionLink("Всички профили", "ViewAllRegisteredProfiles", "Profile")
    </div>
</div>

<h1>Информация за Профила</h1>
<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">Номер</th>
            <th scope="col">Име</th>
            <th scope="col">Телефон</th>
            <th scope="col">Потребителско име</th>
            <th scope="col">Емейл</th>

        </tr>
    </thead>
    <tbody>
        @foreach(var p in Model)
        {
            <tr>
                <td>@p.IDNumber</td>
                <td>@p.Name</td>
                <td>@p.PhoneNumber</td>
                <td>@p.UserName</td>
                <td>@p.Email</td>

            </tr>
        }
    
</tbody>
</table>