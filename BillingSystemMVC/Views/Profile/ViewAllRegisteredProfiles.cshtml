@model IEnumerable<BillingSystemMVC.Model.Profile>
@{
    ViewData["Title"] = "ViewAllRegisteredProfiles";
}

<h1>Профили</h1>
<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">Номер</th>
            <th scope="col">Потребител</th>
            <th scope="col">Потребителски тип</th>
            <th scope="col">Име</th>
            <th scope="col">Телефон</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@p.UserName</td>
                <td>@p.UserNameType</td>
                <td>@p.Name</td>
                <td>@p.PhoneNumber</td>
            </tr>
        }
        
    </tbody>
</table>